{
  "name": "strategy-pattern",
  "version": "1.0.0",
  "description": "Basic repository to training strategy pattern",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/src/app/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "lint": "npx eslint .",
    "prepare": "npx husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "16.1.4",
    "express": "4.18.2",
    "pino": "8.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.42.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "pino-pretty": "10.0.0",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "nodemonConfig": {
    "exec": "ts-node --files -r dotenv/config src/app/index.ts",
    "watch": [
      "src"
    ],
    "ext": "ts"
  }
}
